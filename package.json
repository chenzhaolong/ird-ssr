{
  "name": "vue-ssr-template",
  "version": "1.0.0",
  "description": "vue-ssr-template",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint --ext .js --fix ./client ./server ./entry",
    "publish": "sh ./bin/publish.sh",
    "dll": "sh ./bin/dll.sh",
    "client": "sh ./bin/client.sh",
    "ssr": "cross-env webpack --progress --hide-modules --config ./build/webpack.ssr.js",
    "server:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.js --hide-modules",
    "server:dev": "cross-env NODE_ENV=development node --inspect ./entry/app.server.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenzhaolong/vue-ssr-template.git"
  },
  "keywords": [
    "vue-ssr-template"
  ],
  "author": "dragonchen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chenzhaolong/vue-ssr-template/issues"
  },
  "homepage": "https://github.com/chenzhaolong/vue-ssr-template#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier .prettierrc.js --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "koa": "^2.2.0",
    "koa-router": "^7.4.0",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-router": "^2.8.1",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^2.5.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "colors": "^1.4.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.2.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.15",
    "log4js": "^5.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "sass-loader": "^8.0.0",
    "source-map-support": "^0.5.16",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
